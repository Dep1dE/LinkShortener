services:
  db:
    environment:
      MYSQL_DATABASE: shortlinksdb
      MYSQL_PASSWORD: password
      MYSQL_ROOT_PASSWORD: root
      MYSQL_USER: user
    image: mariadb:10.3
    networks:
      default: null
    ports:
      - mode: ingress
        target: 3306
        published: "3306"
        protocol: tcp
    restart: always
    volumes:
      - type: volume
        source: db_data
        target: /var/lib/mysql
        volume: {}
  linkshortening:
